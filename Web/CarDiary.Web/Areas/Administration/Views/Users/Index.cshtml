@model IEnumerable<UserModel>
<div class="pagetitle">
	<h1>Users</h1>
</div>
<table class="table table-primary text-center">
	<thead>
		<tr>
			<th scope="col">Email</th>
			<th scope="col">CreatedOn</th>
			<th scope="col">Actions</th>
		</tr>	
	</thead>
	<tbody>
		@foreach (var user in this.Model)
		{
			<tr>
				<th>@user.Email</th>
				<th>@user.CreatedOn</th>
				<th>
					<a class="btn btn-outline-danger" asp-action="Delete" asp-route-id="@user.Id">Hard Delete</a>
				</th>
			</tr>
		}
